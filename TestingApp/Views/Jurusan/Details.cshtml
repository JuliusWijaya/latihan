@{
    ViewBag.Title = $"Details {ViewBag.Jrs.Id_Jurusan}";
}

<div class="row">
    <div class="col-12 col-md-8 col-lg-6">
        <div class="card">
            <div class="card-body">
                <div class="mb-3">
                    <label for="Id_Jurusan">ID Jurusan</label>
                    <input class="form-control" name="Id_Jurusan" value="@ViewBag.Jrs.Id_Jurusan" required disabled autofocus autocomplete="off" maxlength="9" />
                </div>

                <div class="mb-3">
                    <label for="Nama_Jurusan">Nama Jurusan</label>
                    <input class="form-control" name="Nama_Jurusan" value="@ViewBag.Jrs.Nama_Jurusan" required disabled autocomplete="off" maxlength="50" />
                </div>

                <div>
                    <a href="@Url.Action("Index", "Jurusan")" class="btn btn-danger mx-2">Kembali</a>
                    <button type="submit" class="btn btn-success">Simpan</button>
                </div>
            </div>
        </div>
    </div>
</div>